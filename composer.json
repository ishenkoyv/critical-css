{
  "name": "ishenkoyv/critical-css",
  "description": "A Laravel package for generating and using inline critical-path CSS.",
  "keywords": ["css", "performance", "laravel"],
  "authors": [
    {
      "name": "Kristoffer Alfheim",
      "email": "kristoffer.alfh@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "illuminate/contracts": "~6.0",
    "illuminate/support": "~6.0",
    "illuminate/console": "~6.0",
    "illuminate/http": "~6.0",
    "illuminate/view": "~6.0",
    "symfony/process": "^2.5 || ^3.0 || ^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Alfheim\\CriticalCss\\": "./src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "config": {
      "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
